{
  "name": "react-notebook",
  "version": "0.0.7",
  "description": "React components for Jupyter Notebook",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerwin/react-notebook.git"
  },
  "keywords": [
    "jupyter",
    "notebook",
    "react"
  ],
  "author": "kerwin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kerwin/react-notebook/issues"
  },
  "homepage": "https://github.com/kerwin/react-notebook#readme",
  "main": "dist/index.js",
  "scripts": {
    "lint": "standard src/*.js src/**/*.js test/*.js test/**/*.js",
    "test": "npm run lint && mocha --compilers js:babel-core/register test/*-spec.js test/**/*-spec.js",
    "watch:test": "mocha -wG --compilers js:babel-core/register test/*-spec.js test/**/*-spec.js",
    "watch:lint": "nodemon --exec \"npm run notify-lint\"",
    "build": "browserify --standalone IPythonNotebook -t [ babelify --presets [react es2015 ] ] src/index.js -o dist/index.js",
    "start": "webpack-dev-server --config example/webpack.config.js --content-base example/"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "sass-loader": "^3.2.0",
    "standard": "^6.0.8",
    "style-loader": "^0.13.0",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "highlight.js": "^9.2.0",
    "json-loader": "^0.5.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-highlight": "^0.7.0",
    "react-remarkable": "^1.1.1",
    "remark": "^4.1.2",
    "remark-react": "^2.0.0",
    "remarkable": "^1.6.2",
    "victorica": "0.0.2"
  }
}